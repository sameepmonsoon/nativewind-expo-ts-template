#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/expo-updates@0.26.19_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtime@5.0.4_react-na_6266fad6c046e1277a4a95b452647495/node_modules/expo-updates/bin/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/expo-updates@0.26.19_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtime@5.0.4_react-na_6266fad6c046e1277a4a95b452647495/node_modules/expo-updates/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/expo-updates@0.26.19_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtime@5.0.4_react-na_6266fad6c046e1277a4a95b452647495/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/expo-updates@0.26.19_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtime@5.0.4_react-na_6266fad6c046e1277a4a95b452647495/node_modules/expo-updates/bin/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/expo-updates@0.26.19_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtime@5.0.4_react-na_6266fad6c046e1277a4a95b452647495/node_modules/expo-updates/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/expo-updates@0.26.19_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtime@5.0.4_react-na_6266fad6c046e1277a4a95b452647495/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi

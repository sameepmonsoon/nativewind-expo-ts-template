#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/jest-expo@52.0.6_@babel+core@7.26.10_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtim_8aece38e7da60e7691e8b61d33d08069/node_modules/jest-expo/bin/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/jest-expo@52.0.6_@babel+core@7.26.10_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtim_8aece38e7da60e7691e8b61d33d08069/node_modules/jest-expo/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/jest-expo@52.0.6_@babel+core@7.26.10_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtim_8aece38e7da60e7691e8b61d33d08069/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/jest-expo@52.0.6_@babel+core@7.26.10_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtim_8aece38e7da60e7691e8b61d33d08069/node_modules/jest-expo/bin/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/jest-expo@52.0.6_@babel+core@7.26.10_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtim_8aece38e7da60e7691e8b61d33d08069/node_modules/jest-expo/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/jest-expo@52.0.6_@babel+core@7.26.10_expo@53.0.8_@babel+core@7.26.10_@expo+metro-runtim_8aece38e7da60e7691e8b61d33d08069/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi

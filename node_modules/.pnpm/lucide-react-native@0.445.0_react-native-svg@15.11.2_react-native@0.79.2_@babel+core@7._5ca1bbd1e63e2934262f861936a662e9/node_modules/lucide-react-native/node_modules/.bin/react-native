#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/react-native@0.79.2_@babel+core@7.26.10_@types+react@18.3.20_react@19.0.0/node_modules/react-native/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/react-native@0.79.2_@babel+core@7.26.10_@types+react@18.3.20_react@19.0.0/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/react-native@0.79.2_@babel+core@7.26.10_@types+react@18.3.20_react@19.0.0/node_modules/react-native/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/react-native@0.79.2_@babel+core@7.26.10_@types+react@18.3.20_react@19.0.0/node_modules:/home/sameep/WEN/Native/nativewind-expo-ts-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../react-native@0.79.2_@babel+core@7.26.10_@types+react@18.3.20_react@19.0.0/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../../../react-native@0.79.2_@babel+core@7.26.10_@types+react@18.3.20_react@19.0.0/node_modules/react-native/cli.js" "$@"
fi
